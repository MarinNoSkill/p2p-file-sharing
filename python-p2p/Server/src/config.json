{
    "server": {
        "host": "0.0.0.0",
        "grpc_port": 50051,
        "rest_port": 8080,
        "max_workers": 10
    },
    "database": {
        "type": "memory",
        "cleanup_interval": 300,
        "peer_timeout": 120
    },
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "file": "server_app.log"
    },
    "security": {
        "enable_auth": true,
        "token_expiry": 3600,
        "max_login_attempts": 3
    }
}